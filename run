#!/bin/bash -x

#-v /usr/lib/x86_64-linux-gnu:/usr/lib/x86_64-linux-gnu:ro \

docker run -it \
	--rm \
	--name jenkins \
	-h salt-master \
	--network salt \
	-p 8080:8080 \
	-p 5000:5000 \
	-p 4505:4505 \
	-p 4506:4506 \
	-v $HOME/tmp/jenkins-home:/var/jenkins_home \
	-v /var/run/docker.sock:/var/run/docker.sock \
	-v $(which docker):/usr/bin/docker \
	-v $HOME/.aws:/root/.aws \
	-v $HOME/.ssh:/root/.ssh \
	--privileged=true \
	test-jenkins
# latest 181107-17.08

